    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.13.0",
    "postcss": "^8.4.38",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "util": "^0.12.5",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^3.0.4",
    "wrangler": "^3.105.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
